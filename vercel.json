{
    "version": 2,
    "framework": null,
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "outputDirectory": "public",
    "routes": [
        {
            "src": "/assets/(.*)/(.*)",
            "dest": "assets/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php?/$1"
        }
    ]
}